    "def fatorial(n):\n",
    "    if(n == 0):\n",
    "        return 1\n",
    "    fat = 1\n",
    "    while(n >= 1):\n",
    "        fat = fat * n\n",
    "        n = n - 1\n",
    "    return fat\n",
    "\n",
    "def fatorial_v2(n):\n",
    "    \n",
    "    if(n == 0):\n",
    "        return 1\n",
    "    valor = 1\n",
    "    fat = 1\n",
    "    while(valor <= n):\n",
    "        fat = fat * valor\n",
    "        valor = valor + 1\n",
    "    return fat\n",
    "\n",
    "def fatorial_v3(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    \n",
    "    fat = 1\n",
    "    for valor in range(1, n+1):\n",
    "        fat = fat * valor\n",
    "    return fat\n",
    "\n",
    "\n",
    "    \n",
    "n = int(input(\"Digite n:\"))\n",
    "print(\"Fatorial de\", n, \"->\", fatorial(n))"
